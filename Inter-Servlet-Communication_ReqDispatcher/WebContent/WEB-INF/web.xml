<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Inter-Servlet-Communication_ReqDispatcher</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
  <servlet-name>ValidationServlet</servlet-name>
  <servlet-class>controller.ValidationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>ValidationServlet</servlet-name>
  <url-pattern>/reg</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>RegistrationServlet</servlet-name>
  <servlet-class>controller.RegistrationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>RegistrationServlet</servlet-name>
  <url-pattern>/next</url-pattern>
  </servlet-mapping>
</web-app>